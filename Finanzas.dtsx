                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT 
                         P.PRESUPUESTO_RUBRO AS FIN_PLAN_CUENTAS_CLAVE, P.PRESUPUESTO_ANIO_FISCAL AS FIN_PSTO_ANIO_FISCAL, P.PRESUPUESTO_PERIODO AS FIN_PSTO_PERIODO, 
                         P.PRESUPUESTO_EJECUTADO AS FIN_PSTO_EJECUTADO, P.PRESUPUESTO_PRESUPUESTADO AS FIN_PSTO_PRESUPUESTADO, P.SOURCE AS FIN_PSTO_SOURCE, 'UYU' AS FIN_PSTO_MONEDA, 
                         CC.CENTRO_COSTO_NOD_ID AS FIN_CC_NOD_ID, CC.CENTRO_COSTO_NOD_DESCR AS FIN_CC_NOD_DESCR, 
                         CASE WHEN CENTRO_COSTO_NOD_DEPTO_TIPO = 'VII-deptos' THEN 'VII-DPTOS' ELSE CENTRO_COSTO_NOD_DEPTO_ID END AS FIN_UNIDAD_OP_CLAVE, P.SOURCE
FROM            dbo.ODS_FIN_PRESUPUESTOS AS P INNER JOIN
                         dbo.ODS_FIN_NDM_CCOSTO AS CCN ON CCN.NODUM_CC_ID = P.PRESUPUESTO_CENTRO_COSTO INNER JOIN
                         dbo.ODS_FIN_CCOSTOS AS CC ON P.PRESUPUESTO_CENTRO_COSTO = CC.CENTRO_COSTO_NOD_ID
WHERE        (P.SOURCE = 'NODUM')
UNION ALL
SELECT        P.PRESUPUESTO_RUBRO AS FIN_PLAN_CUENTAS_CLAVE, P.PRESUPUESTO_ANIO_FISCAL AS FIN_PSTO_ANIO_FISCAL, P.PRESUPUESTO_PERIODO AS FIN_PSTO_PERIODO, 
                         P.PRESUPUESTO_EJECUTADO AS FIN_PSTO_EJECUTADO, P.PRESUPUESTO_PRESUPUESTADO AS FIN_PSTO_PRESUPUESTADO, P.SOURCE AS FIN_PSTO_SOURCE, 'UYU' AS FIN_PSTO_MONEDA, 
                         CAST(CCM.MAGIX_CC_ID AS varchar(50)) AS FIN_CC_NOD_ID, CCM.NODUM_DEPTO_DESCR AS FIN_CC_NOD_DESCR, CCM.MAGIX_CC_DESC_AREA AS FIN_UNIDAD_OP_CLAVE, P.SOURCE
FROM            dbo.ODS_FIN_PRESUPUESTOS AS P INNER JOIN
                             (SELECT DISTINCT CM.MAGIX_CC_ID, CM.MAGIX_CC_ANIO, CM.MAGIX_CC_DESC, CM.MAGIX_CC_DESC_AREA, CN.NODUM_DEPTO_ID, CN.NODUM_DEPTO_DESCR
                               FROM            dbo.ODS_FIN_MGX_CCOSTO AS CM LEFT OUTER JOIN
                                                         dbo.ODS_FIN_NDM_CCOSTO AS CN ON CAST(CM.MAGIX_CC_ID AS varchar) = CN.NODUM_CC_ID_ANTERIOR) AS CCM ON P.PRESUPUESTO_CENTRO_COSTO = CAST(CCM.MAGIX_CC_ID AS varchar) AND 
                         CCM.MAGIX_CC_ANIO = P.PRESUPUESTO_ANIO_FISCAL</property>
